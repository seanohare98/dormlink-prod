{
  "name": "dormlink",
  "version": "1.0.0",
  "description": "A social network for university students to find roommates",
  "scripts": {
    "build": "webpack --env.environment=production --config webpack.config.js",
    "start": "npm run build && node src/server/server.js",
    "client": "webpack --env.environment=production --watch --config webpack.config.js",
    "server": "nodemon src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
